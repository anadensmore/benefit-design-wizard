{
  "functions": {
    "api/health.js": {
      "runtime": "@vercel/node"
    },
    "api/configurations.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/configurations/:path*",
      "destination": "/api/configurations"
    },
    {
      "source": "/api/configurations",
      "destination": "/api/configurations"
    }
  ]
}
